{
  "expo": {
    "name": "Indoor-React-Native",
    "slug": "Indoor-React-Native",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/leaf.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/leaf.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000",
      "plugins": [[
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/pixel-font.ttf"
          ]
        }
      ]]
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "splash": {
        "resizeMode": "native"
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/leaf.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
